{
  "name": "youtube_viewer",
  "version": "0.1.0",
  "author": "TheUnitedStatesOfAmerica",
  "private": true,
  "scripts": {
    "clean": "rimraf dist/",
    "build": "yarn check && webpack -p --progress --display-optimization-bailout --env=prod",
    "build-silent": "yarn check && webpack -p --env=prod",
    "build-force": "yarn check; webpack -p --progress --display-optimization-bailout --env=prod",
    "dev": "yarn && webpack-dev-server --progress --env=dev",
    "dev-silent": "yarn && webpack-dev-server --env=dev",
    "dev-dashboard": "yarn && webpack-dashboard --color blue -- webpack-dev-server --env=dev",
    "test": "jest --watch",
    "test-once": "jest",
    "test-coverage": "jest --coverage",
    "lint": "eslint src"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "<rootDir>/jest.setup.js",
      "<rootDir>/jest.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|styl|sss)$": "<rootDir>/jest.js"
    }
  },
  "devDependencies": {
    "ajv": "~5.3.0",
    "babel-core": "~6.26.0",
    "babel-eslint": "~8.0.0",
    "babel-jest": "~21.2.0",
    "babel-loader": "~7.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "~1.3.0",
    "babel-preset-env": "~1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "~6.24.0",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "0.28.7",
    "enzyme": "~3.1.0",
    "enzyme-adapter-react-16": "~1.0.0",
    "enzyme-to-json": "~3.1.0",
    "eslint": "~4.10.0",
    "eslint-plugin-babel": "~4.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "~7.4.0",
    "html-webpack-plugin": "~2.30.0",
    "jest": "~21.2.0",
    "moment": "^2.22.2",
    "node-sass": "~4.5.0",
    "prop-types": "~15.6.0",
    "react-player": "^1.6.1",
    "react-test-renderer": "~16.0.0",
    "redux-persist": "^5.9.1",
    "rimraf": "~2.6.0",
    "sass-loader": "~6.0.0",
    "style-loader": "0.19.0",
    "webpack": "~3.8.0",
    "webpack-dashboard": "~1.0.0",
    "webpack-dev-server": "~2.9.0",
    "webpack-merge": "~4.1.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/react-fontawesome": "^0.0.20",
    "babel-polyfill": "~6.26.0",
    "bootstrap": "^4.0.0",
    "eslint-config-airbnb": "^16.1.0",
    "file-loader": "~1.1.0",
    "isomorphic-fetch": "~2.2.0",
    "normalize.css": "~7.0.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-hot-loader": "~3.1.0",
    "react-intl": "~2.4.0",
    "react-intl-redux": "0.7.0",
    "react-redux": "~5.0.0",
    "react-router-dom": "~4.2.0",
    "react-router-redux": "5.0.0-alpha.8",
    "redux": "~3.7.0",
    "redux-saga": "0.16.0",
    "sweetalert2": "^7.22.2"
  }
}
